# PostgreSQL
DATABASE_URL=postgresql://user:password@localhost:5432/bbs2

# OpenAI
OPENAI_API_KEY=sk-...

# OpenAI Models (optional - defaults to cost-efficient models)
# Available models:
# - gpt-4o: $0.0081/query (highest quality)
# - gpt-4o-mini: $0.0005/query (recommended - good balance)
# - gpt-3.5-turbo: $0.0015/query (lowest cost, good for testing)
OPENAI_MODEL=gpt-4o-mini
OPENAI_EMBEDDING_MODEL=text-embedding-3-small

# Vector Store
COLLECTION_NAME=bbs_rag_collection